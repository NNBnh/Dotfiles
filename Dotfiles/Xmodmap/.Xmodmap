clear shift
clear control
clear mod1
clear Lock

keycode  64 = Alt_L Meta_L Alt_L Meta_L
keycode  37 = Control_L NoSymbol Control_L
add control = Control_L
add    mod1 = Alt_L Meta_L
add   shift = Shift_L

keycode  61 = Up Up Up
keycode 108 = Left Left Left
keycode 135 = Down Down Down
keycode 105 = Right Right Right

keycode  62 = slash question slash question
keycode 118 = Delete NoSymbol Delete
keycode  66 = Escape NoSymbol Escape
